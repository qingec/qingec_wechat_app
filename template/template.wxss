.text{
  background-color: saddlebrown;
}

.station_fangzi_img{
  width: 25px;
  height: 25px;
  display: inline-block;
}

.station_daohang_img,.station_zhoubian_img{
  width: 15px;
  height: 15px;
  display: inline-block;
}

.station_fangzi_view,.station_daohang_view,.station_zhoubian_view{
  height: 60px;
  text-align: center;
  display: table-cell;
  vertical-align: middle;
}
.station_weizhi_view{
  height: 60px;
  line-height: 60px;
  font-size: 12px;

}
.zhoubian,.daohang{
font-size: 9px;
display: inline-block;
}
.tab_station_detail>view:first-child{
  border-bottom: 1px solid rgba(0, 0, 0, 0.026);
}

.cellrow>view:first-child{
  margin-top: 5px;
}
.cell_title{
  font-size: 12px;
  color: rgba(0, 0, 0, 0.26);
   height: 35px;
   line-height: 35px;
}
.cell_value{
  font-size: 11px;
  color: #000;
   height: 35px;
   line-height: 35px;
}
.cell_value_mini{
  font-size: 10px;
  color: rgba(0, 0, 0, 0.26);
}
.tab_station_group{
  margin-top: 10px;
}
van-cell>view:first-child{
  height: 35px;
  line-height: 35px;
  padding: 0 5px;
}
.tab_station_group van-cell view.van-cell__value{
  height: 35px;
}

.tab_station_group van-cell view.van-cell__right-icon{
  height: 35px;
}

.tab_station_group van-cell van-icon.van-cell__right-icon-wrap {
  height: 35px;
}

.pile_scroll_view{
  height: 24px;
  width: 88%;
  display: inline-block;
  border-bottom: 1px solid rgba(0, 0, 0, 0.26);
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.26);
}

.pile_scroll {
  width: 100%;
  height: 24px;
  white-space: nowrap;
}

.pile_scroll_button{
  display:inline-block;
  height:18px;
  width: auto;
  font-size:10px;
  line-height:18px;
  margin: 3px 2px 3px 2px;
  padding:0px 5px 0px 5px;
  border-radius: 8px;
  color:#1296db;
  background-color:#fff;
  border:1px solid #1296db;
 
}
.pile_scroll_view_shaixuan{
  height: 24px;
  line-height: 24px;
  width: 12%;
  float: right;
  text-align: center;
  display: table;
  border-bottom: 1px solid rgba(0, 0, 0, 0.26);
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.26);
}

.pile_scroll_text_shaixuan{
  font-size: 10px;
  height: 16px;
  line-height: 16px;
  display: table-cell;
  vertical-align: middle;
} 

.pile_scroll_img_shaixuan{
  width: 10px;
  height: 10px;
  /* line-height: 10px; */
  display: inline-block;
  /* vertical-align: middle; */
}
.pile_list_view{
  border-bottom: 1px solid rgba(0, 0, 0, 0.26);
}
.pile_num_short{
  height: 126px;
  line-height: 126px;
  font-size: 15px;
  text-align: center;
}
.pile_num{
  height: 180px;
  line-height: 180px;
  font-size: 15px;
  text-align: center;
}
.pile_info_title{
  display: inline-block;
  text-align: justify;
  text-align-last:justify;
  font-size: 11px;
  width: 44px;
  height: 15px;
  line-height: 15px;
  color: rgba(0, 0, 0, 0.26);
  margin-right: 10px;
}
.pile_info_value{
  display: inline-block;
  font-size: 11px;
  height: 15px;
  line-height: 15px;
}
.pile_info_view view{
  height:15px;
  line-height: 15px;
  margin-bottom: 3px;
}
.pile_status_view{
  height: 126px;
  line-height: 126px;
  font-size: 11px;
  text-align: center;
}
.pile_end_view{
  height: 100px;
  line-height: 100px;
  font-size: 11px;
  display: block;
  text-align: center;
}


.pile_fresh_view{
  font-size: 11px;
  text-align: center;
}

.pile_status_outline{
  color: #fff;
  background-color: rgba(0, 0, 0, 0.26);
  border-radius: 3px;
  padding: 1px;
  margin-right: 2px;
}

.pile_status_online{
  color: #fff;
  background-color: rgba(100, 248, 1, 0.781);
  border-radius: 3px;
  padding: 1px;
  margin-right: 2px;
}

.pile_status_charging{
   color: #fff;
  background-color: rgba(0, 255, 242, 0.747);
  border-radius: 3px;
  padding: 1px;
  margin-right: 2px;
}

.pile_status_error{
   color: #fff;
  background-color: rgba(255, 0, 0, 0.822);
  border-radius: 3px;
  padding: 1px;
  margin-right: 2px;
}
.pile_status_wait{
   color: #fff;
  background-color: rgba(0, 60, 255, 0.822);
  border-radius: 3px;
  padding: 1px;
  margin-right: 2px;
}

.station_list_shoucang_no_img{
  height: 21px;
  width: 21px;
  float: right;
  margin:2px 10px 2px auto;
}
