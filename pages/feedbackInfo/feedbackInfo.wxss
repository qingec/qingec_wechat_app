/* pages/feedbackInfo/feedbackInfo.wxss */
.feedback_msg_area{
  height: 100px;
  width: 85vw;
  background-color: rgba(0, 0, 0, 0.026);
  border-radius: 10px;
  margin:10px auto;
  font-size: 10px;
  padding: 10px;
}

.feedback_msg_placeholder{
  font-size: 10px;
  color: rgba(0, 0, 0, 0.35);
}
.feedback_tishi_view .van-cell{
  margin: 0px 10px;
  border-bottom: 1px solid  rgba(0, 0, 0, 0.26);
}
.photo_img_view{
  background-color: rgba(0, 0, 0, 0.050);
  width: 80px;
  height: 80px;
  margin: 10px;
  border-radius: 3px;
}
.feedback_photo_img{
  height: 70px;
  width: 70px;  
  margin: 5px;
}

.btn_feedback {
  margin-top: 60rpx;
  background: #1296db;
  color: #fff;
  width: 90%;
  position: fixed;
  bottom: 10px;
  text-align: center;
  left: 0;
  right: 0;
}

.btn_view{
  height: 50vh;
  line-height: 50vh;
  
  
}