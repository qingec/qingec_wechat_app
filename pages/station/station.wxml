  </van-col>
</van-row>

<van-row class='map_screen_div'>
  <!-- 滑块 -->
  <view class='scrollContainer_view'>
    <scroll-view class='scrollContainer' scroll-x>
      <button type='button' class='scrollContainer_button'>
        精品站</button>
      <button type='button' class='scrollContainer_button'>免费停车</button>
      <button type='button' class='scrollContainer_button'>自营</button>
      <button type='button' class='scrollContainer_button'>交流</button>
      <button type='button' class='scrollContainer_button'>直流</button>
      <button type='button' class='scrollContainer_button'>测试</button>
      <button type='button' class='scrollContainer_button'>测试</button>
      <button type='button' class='scrollContainer_button'>测试</button>
      <button type='button' class='scrollContainer_button'>测试</button>
      <button type='button' class='scrollContainer_button'>测试</button>
    </scroll-view>
  </view>
  <!-- 排序 -->
  <view class='scrollContainer_view_paixu'>
    <text class='scrollContainer_text_paixu'>排序</text>
    <image class='scrollContainer_img_paixu' src='/image/paixu.png'></image>
  </view>
  <!-- 筛选 -->
  <view class='scrollContainer_view_shaixuan'>
    <text class='scrollContainer_text_shaixuan'>筛选</text>
    <image class='scrollContainer_img_shaixuan' src='/image/shaixuan.png'></image>
  </view>
</van-row>

