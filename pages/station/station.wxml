<van-row class='map_search_div'>
  <van-col span='21'>
    <van-search value="{{ value }}" placeholder="请输入关键词"></van-search>
  </van-col>
  <van-col span='3'>
    <!-- <cover-image class='map_lists_img' src='/image/map_lists.png'></cover-image> -->
